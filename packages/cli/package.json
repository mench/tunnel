{
  "name": "@relcu/tunnel-cli",
  "version": "1.2.0",
  "description": "Tunnel Cli",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@relcu/tunnel-client": "*",
    "@relcu/tunnel-server": "*",
    "@types/optimist": "^0.0.29",
    "optimist": "^0.6.1"
  },
  "devDependencies": {
    "@types/node": "^11.13.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.4.2"
  },
  "bin": {
    "tunnel": "./bin/tunnel.js"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "connect": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node ./src/index connect --domain=tunnel.relcu.dev --secure=true --auth=armen:'$123456$' --host=client.quz.li --port=443 --subdomain=gago",
    "serve": "ts-node ./src/index serve",
    "release": "npm publish"
  }
